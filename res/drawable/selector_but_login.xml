<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <stroke android:width="1px" android:color="#64BBFF" />

            <corners android:radius="3dp" />

            <gradient android:angle="90" android:endColor="#7DC6FF" android:startColor="#73C1FD" />
        </shape></item>
    <item><shape>
            <stroke android:width="1px" android:color="#64BBFF" />

            <corners android:radius="3dp" />

            <gradient android:angle="90" android:endColor="#73C1FD" android:startColor="#7DC6FF" />
        </shape></item>

</selector>